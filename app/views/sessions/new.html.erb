<p id="notice"><%= notice %></p>
<h1>Sign in</h1>

<%= form_tag session_path do %>
  <div align="center">
    <label>Username <%= text_field_tag :username, params[:username] %>
    Password <%= password_field_tag :password, params[:password] %>
    <%= submit_tag "Login", class: 'button'%>
    </label>
  </div>
<% end %>
